   "source": [
    "from collections import deque\n",
    "n = int(input())\n",
    "x = deque()\n",
    "\n",
    "for i in range(1,n+1):\n",
    "    x.append(i)\n",
    "\n",
    "for _ in range(n-1):\n",
    "    x.popleft()\n",
    "    x.rotate(-1)\n",
    "\n",
    "print(x[0])\n"
   ]
